#! /usr/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x2538290 .scope module, "TOGGLE_DATA_8B_tb" "TOGGLE_DATA_8B_tb" 2 3;
 .timescale -9 -12;
v0x254b320_0 .var "clk", 0 0;
v0x254b3e0_0 .net "data", 7 0, v0x254afe0_0;  1 drivers
v0x254b4b0_0 .var "en", 1 0;
v0x254b5b0_0 .var "rstn", 0 0;
S_0x2538410 .scope module, "uut" "TOGGLE_DATA_8B" 2 14, 3 1 0, S_0x2538290;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rstn"
    .port_info 2 /INPUT 2 "en"
    .port_info 3 /OUTPUT 8 "data"
v0x2534690_0 .net "clk", 0 0, v0x254b320_0;  1 drivers
v0x254afe0_0 .var "data", 7 0;
v0x254b0c0_0 .net "en", 1 0, v0x254b4b0_0;  1 drivers
v0x254b1b0_0 .net "rstn", 0 0, v0x254b5b0_0;  1 drivers
E_0x2534be0/0 .event negedge, v0x254b1b0_0;
E_0x2534be0/1 .event posedge, v0x2534690_0;
E_0x2534be0 .event/or E_0x2534be0/0, E_0x2534be0/1;
    .scope S_0x2538410;
T_0 ;
    %wait E_0x2534be0;
    %load/vec4 v0x254b1b0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x254afe0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x254b0c0_0;
    %cmpi/ne 0, 0, 2;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x254afe0_0;
    %cmpi/e 0, 0, 8;
    %flag_mov 8, 4;
    %jmp/0 T_0.4, 8;
    %pushi/vec4 255, 0, 8;
    %jmp/1 T_0.5, 8;
T_0.4 ; End of true expr.
    %pushi/vec4 0, 0, 8;
    %jmp/0 T_0.5, 8;
 ; End of false expr.
    %blend;
T_0.5;
    %assign/vec4 v0x254afe0_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x2538290;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x254b320_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x254b320_0;
    %inv;
    %store/vec4 v0x254b320_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x2538290;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x254b5b0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x254b4b0_0, 0, 2;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x254b5b0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x254b4b0_0, 0, 2;
    %delay 20000, 0;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x254b4b0_0, 0, 2;
    %delay 20000, 0;
    %delay 10000, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x254b4b0_0, 0, 2;
    %delay 20000, 0;
    %delay 10000, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x254b4b0_0, 0, 2;
    %delay 20000, 0;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x254b5b0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x254b5b0_0, 0, 1;
    %delay 50000, 0;
    %vpi_call 2 55 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x2538290;
T_3 ;
    %vpi_call 2 59 "$monitor", "Time: %0dns, En: %b,  Toggled Data: %h", $time, v0x254b4b0_0, v0x254b3e0_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "TB_TOGGLE_DATA_8B.v";
    "TOGGLE_DATA_8B.v";
